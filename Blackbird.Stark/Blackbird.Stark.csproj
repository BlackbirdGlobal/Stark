<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageVersion>1.0.6</PackageVersion>
    <Title>Blackbird.Stark</Title>
    <Authors>Artem Drozdov</Authors>
    <Description>Handy Computer Science algorithms and data structures.</Description>
    <PackageProjectUrl>https://github.com/Blackbird-tech/Stark</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Blackbird-tech/Stark</RepositoryUrl>
    <Copyright>Artem Drozdov</Copyright>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseUrl>https://raw.githubusercontent.com/Blackbird-tech/Stark/master/License.md</PackageLicenseUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageIconUrl>https://raw.githubusercontent.com/Blackbird-tech/Stark/master/icon.png</PackageIconUrl>
    <LangVersion>10</LangVersion>
  </PropertyGroup>
  
</Project>
